    "version": "0.2.0",
    "configurations": [
        {
            "name": "school App",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=ENVIRONMENT=DEV"
            ]
        },
        {
            "name": "school App (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=ENVIRONMENT=STAGING"
            ]
        },
        {
            "name": "school App (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=ENVIRONMENT=PRODUCTION"
            ]
        }
    ]
}